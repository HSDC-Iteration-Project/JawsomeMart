{
  "name": "jawsomemart",
  "version": "1.0.0",
  "description": "An e-commerce application",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000 --forceExit --detectOpenHandles"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/frontend/test/sum.test.jsx"
    ],
    "globalSetup": "./jest-setup.js"
  },
  "keywords": [
    "MERN",
    "Express",
    "React",
    "Node",
    "Redux"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "jest-cli": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2",
    "react-router-dom": "^6.26.0",
    "stripe": "^16.6.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "vite": "^5.4.0"
  }
}
